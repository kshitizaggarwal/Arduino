void MYon()
{
  layer1on();
  //layer2on();
  //layer3on();
  Mon();
  delay(d);
  layer1off();
  layer2off();
  layer3off();
  delay(d);
  
  layer1on();
 // layer2on();
 // layer3on();
  Yon();
  delay(d);
  layer1off();
  layer2off();
  layer3off();
  delay(d);
}
